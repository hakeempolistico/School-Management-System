CREATE TABLE `school_management`.`enroll` ( `id` INT(11) NOT NULL AUTO_INCREMENT , `lrn` INT(50) NOT NULL , `firstname` VARCHAR(50) NOT NULL , `middlename` VARCHAR(50) NOT NULL , `lastname` VARCHAR(50) NOT NULL , `sex` VARCHAR(10) NOT NULL , `contactno` INT(20) NOT NULL , `birthdate` DATE NOT NULL , `birthplace` VARCHAR(50) NOT NULL , `age` INT(4) NOT NULL , `mothertongue` VARCHAR(50) NOT NULL , `religion` VARCHAR(50) NOT NULL , `street` VARCHAR(50) NOT NULL , `barangay` VARCHAR(50) NOT NULL , `city` VARCHAR(50) NOT NULL , `province` VARCHAR(50) NOT NULL , `fathername` VARCHAR(50) NOT NULL , `mothername` VARCHAR(50) NOT NULL , `fathercontact` INT(20) NOT NULL , `mothercontact` INT(20) NOT NULL , `guardian` VARCHAR(50) NOT NULL , `relationship` VARCHAR(50) NOT NULL , `guardiancontact` INT(20) NOT NULL , PRIMARY KEY (`id`), UNIQUE (`lrn`)) ENGINE = MyISAM;